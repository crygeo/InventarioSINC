<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Cliente.ViewModel"
                    xmlns:view="clr-namespace:Cliente.View"
                    xmlns:model="clr-namespace:Cliente.View.Model"
                    xmlns:subPage="clr-namespace:Cliente.src.View.Model.SubPage"
                    xmlns:mvvm="clr-namespace:Utilidades.Mvvm;assembly=Utilidades">
    
    
    
    <DataTemplate DataType="{x:Type vm:PageProcesosVM}">
        <view:PageProcesosV/>
    </DataTemplate>

    <DataTemplate x:Key="ViewTemplate_Rol">
        <model:PageRolesV/>
    </DataTemplate>

    <DataTemplate x:Key="ViewTemplate_Usuario">
        <model:PageUsuariosV/>
    </DataTemplate>

    <DataTemplate x:Key="ViewTemplate_ElementoJerarquico">
        <model:PageValoresV/>
    </DataTemplate>

    <DataTemplate x:Key="IdentificadoresTemplate" DataType="{x:Type mvvm:SubViewModelBase}">
        <subPage:PageIdentificadoresV />
    </DataTemplate>


</ResourceDictionary>